@{
	Layout = null;
}
<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!-- 上述3个meta标签*必须*放在最前面，任何其他内容都*必须*跟随其后！ -->
	<title>Bootstrap 101 Template</title>
	<!-- Bootstrap -->
	<link href="//cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
	<link href="/Content/CSS/Home/Index.css" rel="stylesheet">
	<!--[if lt IE 9]>
	  <script src="https://cdn.bootcss.com/html5shiv/3.7.3/html5shiv.min.js"></script>
	  <script src="https://cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
	<![endif]-->
</head>
<body>
	<div class="header">
		<div class="header-main">
			<nav class="navbar navbar-inverse" style="height:36px;">
				<div class="container-fluid">
					<!-- Brand and toggle get grouped for better mobile display -->
					<div class="navbar-header">
						<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</button>
						<a class="navbar-brand" href="#">Brand</a>
					</div>
					<!-- Collect the nav links, forms, and other content for toggling -->
					<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
						<ul class="nav navbar-nav">
							<li class="active"><a href="#">Link <span class="sr-only">(current)</span></a></li>
							<li><a href="#">Link</a></li>
							<li class="dropdown">
								<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown <span class="caret"></span></a>
								<ul class="dropdown-menu">
									<li><a href="#">Action</a></li>
									<li><a href="#">Another action</a></li>
									<li><a href="#">Something else here</a></li>
									<li role="separator" class="divider"></li>
									<li><a href="#">Separated link</a></li>
									<li role="separator" class="divider"></li>
									<li><a href="#">One more separated link</a></li>
								</ul>
							</li>
						</ul>
						<form class="navbar-form navbar-right">
							@*<div class="form-group">
									<input type="text" class="form-control" placeholder="Search">
								</div>*@
							<div class="input-group">
								<span class="input-group-btn">
									<input type="text" class="form-control" placeholder="Search">
									<span class="input-group-btn">
										@*<button class="btn btn-default" type="button">Go!</button>*@
										<button type="submit" class="btn btn-default">
											<span class="glyphicon glyphicon-search" aria-hidden="true"></span>
										</button>
									</span>
								</span>

							</div>

						</form>
						<img class="img-circle center-block navbar-right" style="width:35px;height:35px;margin-top:7px;" src="/Content/Image/defaultImage.jpg">
					</div><!-- /.navbar-collapse -->
				</div><!-- /.container-fluid -->
			</nav>
		</div>
	</div>
	<div class="main">
		<div class="container-fluid">
			<div class="row">
				<div class="col-md-9" style=" ">
					@*height: 0px; padding-bottom: 100%*@
					<p>9 人在线 - 1 会员, 8 游客 共262位会员</p>
					<div class="row">
						<div class="col-md-8">
							<div class="btn-group">
								<button type="button" class="btn btn-default active">最新</button>
								<button type="button" class="btn btn-default">最新回复</button>
								<button type="button" class="btn btn-default">我发</button>
								<button type="button" class="btn btn-default">我回</button>
								<button type="button" class="btn btn-default">
									<span class="glyphicon glyphicon-star" aria-hidden="true"></span> 收藏
								</button>
							</div>
						</div>
						<div class="col-md-2 col-md-offset-2" style="">
							<button type="button" class="btn btn-success">
								<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> 发布
							</button>
						</div>
					</div>
					<ul class="list">
						@for (var i = 0; i < 25; i++)
						{
							<li class="list-li" style="">
								<a class="list-li-image" href="#"><img class="img-rounded" style="width:45px;height:45px;margin-top:2px;" src="/Content/Image/defaultImage.jpg"></a>
								<a class="list-li-title " href="#">[反馈] 反馈BUG</a>
								<p class="list-li-otherinfo ">
									<span class="otherinfo-span">Aaron 阿婆</span>
									<span class="otherinfo-span">4天前</span>
									<span class="otherinfo-span absolute-right ">
										<i class="glyphicon glyphicon-comment" style="margin-left:10px" aria-hidden="true"></i> 6
										<i class="glyphicon glyphicon-eye-open" style="margin-left:10px" aria-hidden="true"></i> 134
									</span>
								</p>
							</li>
						}

					</ul>
					<div class="row">
						@*<div class="col-md-3" ><span>总共:195 条 当前:1/8</span>	</div>*@
						<div class="col-md-12" >
							<nav aria-label="Page navigation">
								<span style="height:100%;line-height:100%;">总共:195 条 当前:1/8</span>
								<ul class="pagination">
									<li>
										<a href="#" aria-label="Previous">
											<span aria-hidden="true">&laquo;</span>
										</a>
									</li>
									<li><a href="#">1</a></li>
									<li class="active"><a href="#">2</a></li>
									<li><a href="#">3</a></li>
									<li><a href="#">4</a></li>
									<li><a href="#">5</a></li>
									<li><a href="#">6</a></li>
									<li><a href="#">7</a></li>
									<li><a href="#">8</a></li>
									<li><a href="#">9</a></li>
									<li><a href="#">10</a></li>

									<li>
										<a href="#" aria-label="Next">
											<span aria-hidden="true">&raquo;</span>
										</a>
									</li>
								</ul>
							</nav>
						</div>
					</div>


					


					

				</div>
				<div class="col-md-3" style="background-color: #f2f2f2; ">.col-md-3</div> @*height: 0px; padding-bottom: 100%;*@
			</div>
		</div>
	</div>
	<div class="footer">
		我是Footer
	</div>








	<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
	<script src="//cdn.bootcss.com/jquery/3.2.1/jquery.min.js"></script>
	<!-- Include all compiled plugins (below), or include individual files as needed -->
	<script src="//cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</body>
</html>
